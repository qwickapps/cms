{
  "name": "photography-portfolio-example",
  "version": "1.0.0",
  "private": true,
  "description": "Photography portfolio example using @qwickapps/cms",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "seed": "tsx seed/data.ts",
    "setup": "./setup.sh",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d && echo '✓ PostgreSQL running on port 5433'",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v && echo '✓ Database volume removed'",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d && sleep 3 && npm run seed && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@payloadcms/db-postgres": "3.65.0",
    "@payloadcms/live-preview-react": "^3.65.0",
    "@payloadcms/next": "3.65.0",
    "@payloadcms/richtext-lexical": "3.65.0",
    "@payloadcms/ui": "3.65.0",
    "@qwickapps/cms": "file:../..",
    "@qwickapps/react-framework": "^1.5.0",
    "@qwickapps/schema": "^1.3.2",
    "next": "15.5.7",
    "payload": "3.65.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/node": "^20.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "tsx": "^4.19.0",
    "typescript": "^5.3.3"
  }
}
